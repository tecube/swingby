{
  "name": "Swing by",
  "description": "Shows an context menu for storing selected words",
  "version": "0.0.1",
  "permissions": [
    "contextMenus",
    "storage",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_title": "swingby",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  "manifest_version": 2
}